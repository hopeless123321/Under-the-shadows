; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false
resources_spreadsheet_view/fold_docks=true
resources_spreadsheet_view/resource_cell_label_mode=1
resources_spreadsheet_view/freeze_first_column=true

[application]

config/name="Under the shadow"
config/version="0.06"
run/main_scene="res://UI/Scene/Main menu/Scene/Main menu.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)

[autoload]

Overlay="*res://Global scripts/Global Overlay.tscn"
GlobalInfo="*res://Global scripts/Global script/Global info.gd"
Eventbus="*res://Global scripts/Global script/Eventbus.gd"
Grid="*res://Global scripts/Global script/Grid.gd"
Config="*res://Global scripts/Config.gd"
UnitManager="*res://Global scripts/Global script/Unit manager.gd"
SkillManager="*res://Global scripts/Global script/Skill manager.gd"
Translater="*res://Global scripts/Global script/Translater.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=680
mouse_cursor/custom_image="res://UI/UI assets/Custom mouse/cursour.png"
mouse_cursor/tooltip_position_offset=Vector2(30, 30)

[editor_plugins]

enabled=PackedStringArray("res://addons/any_icon.editor/plugin.cfg", "res://addons/bbcode_edit.editor/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg")

[file_customization]

folder_colors={
"res://Resources/Abilities/": "teal",
"res://Resources/Locations/": "blue",
"res://Resources/Relics/": "purple",
"res://Resources/Relics/Drop after Elite Battle/": "red",
"res://Resources/Relics/Drop from Boss/": "orange",
"res://Resources/Relics/Drop from Event/": "yellow",
"res://Resources/Relics/Drop from Shop/": "green",
"res://Resources/TileSets/": "teal",
"res://Resources/Unit lists/": "pink",
"res://Resources/Units/": "gray",
"res://Scenes/Battle scenes/Unit/State machine Ally/": "green",
"res://Scenes/Battle scenes/Unit/State machine Enemy/": "red"
}

